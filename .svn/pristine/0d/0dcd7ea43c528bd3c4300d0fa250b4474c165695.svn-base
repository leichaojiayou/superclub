
<!--pages/cash6/cash6.wxml-->
<view class="item" wx:for="{{item}}">

  <!--第一行-->
  <view class="item_1" bindtap="nato_cash7">
    <text class="t32rpx">{{item.txt}}</text>
    <text class="t38rpx">{{item.money}}</text>
  </view>

  <!--第二行-->
  <view class="item_1" bindtap="nato_cash7">
  <text style="color: rgb(142, 142, 142);" class="t24rpx">{{item.time}}</text>
  <text style="color: {{item.color}}" class="t24rpx">{{item.statu}}</text>
  </view>
</view>