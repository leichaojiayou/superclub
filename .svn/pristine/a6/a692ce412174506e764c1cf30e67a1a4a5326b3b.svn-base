      }
    ],
    //第二个模板
    acts: [],
    item3: [{
      img: '',
      name: '潍坊市马拉松运动协会2017黄河口东营-国际马拉松赛报名啊啊啊啊啊啊啊啊啊啊啊啊',
      time: '2017.12.21 17:30',
