/* pages/dismiss/dismiss.wxss */
page{
    white-space: normal;
}
.header{
    display: flex;
    justify-content: center;
    padding-top: 20rpx;
    margin-top: 99rpx;
}
.header_img{
    height: 130rpx;
    width: 130rpx;
    border-radius: 10rpx; 

}
.header_t{
    display: flex;
    justify-content: center;
    margin-top: 48rpx;
}
.header_btn{
    margin: 60rpx 30rpx 0 30rpx;
}
.appoint-list{
    padding-top: 30rpx;
}
.appoint-list-item{
    display: inline-block;
    width:33%;
    text-align: center;
}
.appoint-list-title{
    font-size:32rpx;
    letter-spacing:-0.8rpx;
    display: block;
    word-break:keep-all;
    overflow:hidden;
    text-overflow:ellipsis;
    padding: 20rpx 0;
}