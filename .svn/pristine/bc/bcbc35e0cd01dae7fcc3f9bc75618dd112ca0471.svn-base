.big_view {
  background-color: rgb(255, 255, 255);
  font-size: 32rpx;
  margin-top: 31rpx;
  /*border-bottom: 1rpx solid rgb(230, 230, 230);
  border-top: 1rpx solid rgb(230, 230, 230);*/
}

.item_view {
  border-top: 1rpx solid rgb(230, 230, 230);
  border-bottom: 1rpx solid rgb(230, 230, 230);
  display: flex;
  flex-direction: column;
  align-items: stretch;
  justify-content: flex-start;
  line-height: 44rpx;
}

.item_top_view {
  display: flex;
  justify-content: space-between;
  align-items: baseline;
  margin-top: 29rpx;
  margin-left: 29rpx;
  margin-right: 30rpx;
}

.item_mid_view {
  display: flex;
  justify-content: space-between;
  align-items: baseline;
  /*margin-top: 19rpx;*/
  margin-left: 30rpx;
  margin-right: 29rpx;
}

.item_bottom_view {
  display: flex;
  justify-content: space-between;
  align-items: baseline;
  /*margin-top: 21rpx;*/
  margin-left: 30rpx;
  margin-bottom: 28rpx;
  margin-right: 30rpx;
}

.bigsize_text {
  color: rgb(26, 26, 26);
  font-size: 32rpx;
}

.midsize_text {
  color: rgb(142, 142, 142);
