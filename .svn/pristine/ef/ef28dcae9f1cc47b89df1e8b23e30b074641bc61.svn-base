<import src="../../../template/common/common.wxml" />
<!--头部-->
<view class="head" bindtap="navme">

  <!--头像-->
  <image class="head_img" mode="aspectFill" src="{{msg.head_img}}"></image>
  <view class="head_txt"  bindtap="nato_myaccount">

    <!--名称-->
    <text class="head_name">{{msg.name}}</text>
    <text class="des_txt">{{msg.head_signname}}</text>
  </view>

  <!--右箭头-->
  <image src="../../../assets/images/right-errow.png" class="right_errow" mode="aspectFill"></image>
</view>

<!--中间-->
<view class="container1" style="position: relative;"  bindtap="addClub">

  <view class="mycharge">
<text class="mycharge_txt">我管理的</text>
</view>
<view class="mycharge1">
    <!--状态 1 未管理任何俱乐部-->
    <text wx:if="{{statu==1}}" class="des_txt">您未管理任何俱乐部，点击创建俱乐部</text>

    <!--状态 2 审核状态-->
    <text wx:elif="{{statu==2}}" class="des_txt">您创建的俱乐部"{{clubname}}"正在审核中，请耐心等待</text>

    <!--状态 3 管理的俱乐部-->
    <view wx:elif="{{statu==3}}" class="manage">

      <!--空白 暂未完成-->

  </view>
</view>
  <!--右箭头-->
  <image src="../../../assets/images/right-errow.png" class="right_errow" mode="aspectFill"></image>
</view>

<!--中间-->
<view class="container1" style="position: relative;"  bindtap="manageClub">

  <view class="mycharge">
  <text class="mycharge_txt">我管理的</text>
  </view>

  <view class="mycharge1">
      <text class="des_txt">xxx俱乐部</text>
  </view>
  
  <!--右箭头-->
  <image src="../../../assets/images/right-errow.png" class="right_errow" mode="aspectFill"></image>
</view>


<!--底部容器-->
<!--我加入的-->
<view class="container">
  <view class="container_title">
    <text>我加入的</text>
  </view>
  <view bindtap="nato_clubhome">
      <template is="common_one" data="{{item,item1}}" bindtap="clubhome"/>
  </view>   
</view>

<!--最近访问-->
<view class="container">
  <view class="container_title">
    <text>最近访问</text>
  </view>
  <view bindtap="nato_clubhome">
    <template is="common_one" data="{{item,item1}}"/>
  </view>
  <view bindtap="nato_clubhome">
    <template is="common_one" data="{{item,item1}}"/>
  </view>
</view>

<view style="height:4.5vh;backgroundcolor:rgb(246,246,246)"></view>