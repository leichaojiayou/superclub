@import "../../../template/common/common.wxss";
@import "../../../template/toasts/toasts.wxss";
@import "../../club/clubhome/clubhome.wxss";

.back_head {
  background-color: rgb(89, 215, 176);
}

.back_top {
  margin-bottom: 17px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}


.club_msg {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin-left: 30rpx;
}

.back_foot,
.tran {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  position: relative;
}

.back_foot {
  margin: 20px 0 20px 0;
}

.line {
  position: absolute;
  width: 80px;
  border-bottom: 1px solid rgb(230, 230, 230);
  top: 9px;
}

.label_txt {
  color: rgb(58, 157, 126);
  font-size: 19px;
  margin-bottom: 5px;
}

.xiegan {
  position: absolute;
  right: 67%;
  top: 15%;
}

.xiegan2 {
  position: absolute;
  right: 33.3%;
  top: 15%;
}

.point {
  background: red;
  width: 40rpx;
  height: 40rpx;
  border-radius: 50px;
  position: absolute;
  /*margin-left: 30rpx;
  margin-top: 50rpx;*/
  top: 50rpx;
  right: 0rpx;
  text-align: center;
  line-height: 40rpx;
}

.body_item1 {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}

.list_img {
  width: 78rpx;
  height: 78rpx;
  opacity: 30%;
}
.nd_img_class{
  width: 112px;
  height: 77px;
}
.common2 {
  border-bottom: 1px solid rgb(246, 246, 246);
  position: relative;
  margin-bottom: 0;
  padding-bottom:0;
}
.freemsg-position{
  position: absolute;
  right: 0;
  bottom: 0;
}
.nd_header{
  margin: 0;
  padding: 0 20rpx;
}
.mangage-money{
  position: absolute;
  right: 20rpx;
  bottom: 0;
}
.manage-list-bg{
  background: #fff;
}
.manage-club-logo{
  width: 140rpx;
  height: 140rpx;
  border-radius: 10rpx;
  margin: 20rpx 20rpx 20rpx 60rpx;
  background: none;
}
.manage-club-content{
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
}
.manage-club-errow{
  width: 36rpx;
  height: 38rpx;
  padding-right: 20rpx;
}
.manage-club-text{
  padding-top: 10rpx;
  font-size: 28rpx;
  display: block;
}


.midview_list{
  padding-bottom: 20rpx;
}