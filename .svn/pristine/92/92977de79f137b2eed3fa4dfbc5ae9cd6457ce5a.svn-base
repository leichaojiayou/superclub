
<!--pages/julebuzhuye/julebuzhuye.wxml-->
<view class="header">

  <!--line1-->
  <view class="line1">
    <text class="t34rpx">我管理的</text>
    <view class="greenitem">
      <text class="t26rpx_green">管理俱乐部</text>
    </view>
  </view>

  <!--line1 e-->

  <!--line2-->
  <view class="line2">
    <image src="../images/chef.png" class="line2_img"></image>
    <text class="t26rpx_grey">担任会长职务</text>
  </view>

  <!--line2 e-->

  <!--line3-->
  <view class="line3">

    <!--头像-->
    <image src="../images/user.png" class="line3_img"></image>

    <!--骑行 徒步-->
    <view class="line3_mid">
      <text>xxxxxxxx俱乐部</text>
      <view class="xunhuanyangshi">
        <view wx:for="{{item}}">
          <view class="kuangkuang" style="background-color:{{item.color}};">
            <text class="t26rpx_white">{{item.sort}}</text>
          </view>
        </view>

        <!--箭头-->
        <image src="../images/right-errow.png" class="arrow"></image>
      </view>
      <text class="t26rpx_grey">3个新活动</text>
    </view>
  </view>

  <!--line3 e-->
</view>

