
<!--pages/cash/cash.wxml-->
<view class="line1" bindtap="nato_cash3_1">

  <!--设置提现用户-->
  <text style="font-size:32rpx;line-height:36rpx;letter-spacing:-0.8rpx;">{{line1}}</text>
  <image src="https://cdn.51julebu.com/xiaochengxu/image/right-errow.png" class="line1_img"></image>
</view>
<view class="line2">

  <!--金额-->
  <text style="font-size:32rpx;line-height:36rpx;letter-spacing:-0.8rpx;">{{line2}}</text>
  <text style="font-size:32rpx;line-height:36rpx;letter-spacing:-0.8rpx;color:rgb(191,191,191)">可提现金额{{line2money}}</text>
</view>

<!--每天一次，单笔~-->
<view class="line2_bottom">
  <text style="font-size:26rpx;line-height:36rpx;letter-spacing:-0.6rpx;color:rgb(142,142,142)">每天一次，单笔最高1万元。</text>
  <text style="font-size:26rpx;line-height:36rpx;letter-spacing:-0.8rpx;color:rgb(61,209,164)">提现规则</text>
</view>
<view hidden="{{false}}">
  <view class="line3">

    <!--真实姓名-->
    <text style="font-size:32rpx;line-height:36rpx;letter-spacing:-0.8rpx;">真实姓名</text>
    <input placeholder="输入" class="input" placeholder-style="font-size:32rpx;line-height:36rpx;letter-spacing:-0.8rpx;color:rgb(191,191,191)" maxlength="10" bindinput="inputRealName" />
  </view>

  <!--身份证正面~手持本人身份证拍照-->
  <view class="body4">

    <!--身份证正面-->
    <view class="body4_1" bindtap="take_front_img">
      <text style="font-size:32rpx;line-height:36rpx;letter-spacing:-0.8rpx;">身份证正面</text>
      <image wx:if="{{frontImgPath==null || frontImgPath==''}}" src="https://cdn.51julebu.com/xiaochengxu/image/sfzzm@2x.png" class="body4_1img" mode="aspectFill"></image>
      <image wx:else src="{{frontImgPath}}" class="body4_1img" mode="aspectFill"></image>
      <image src="https://cdn.51julebu.com/xiaochengxu/image/right-errow.png" class="line1_img1"></image>
    </view>

    <!--身份证反面-->
    <view class="body4_2" bindtap="take_back_img">
      <text style="font-size:32rpx;line-height:36rpx;letter-spacing:-0.8rpx;">身份证反面</text>
      <image wx:if="{{backImgPath==null || backImgPath==''}}" src="https://cdn.51julebu.com/xiaochengxu/image/sfzfm@2x.png" class="body4_1img" mode="aspectFill"></image>
      <image wx:else src="{{backImgPath}}" class="body4_1img" mode="aspectFill"></image>
      <image src="https://cdn.51julebu.com/xiaochengxu/image/right-errow.png" class="line1_img1"></image>
    </view>

    <!--手持本人身份证拍照-->
    <view class="body4_3" bindtap="take_person_img">
      <text style="font-size:32rpx;line-height:36rpx;letter-spacing:-0.8rpx;">手持本人身份证拍照</text>
      <image hidden="{{personImgPath==null || personImgPath==''}}" src="{{personImgPath}}" class="body4_1img" mode="aspectFill"></image>
      <image src="https://cdn.51julebu.com/xiaochengxu/image/right-errow.png" class="line1_img1"></image>
    </view>

    <!--e-->
  </view>

  <!--身份认证后不可修改，审核通过后即可提现。-->
  <view style="margin:16rpx 0 0 30rpx;">
    <text style="font-size:26rpx;line-height:36rpx;letter-spacing:-0.6rpx;color:rgb(142,142,142)">身份认证后不可修改，审核通过后即可提现。</text>
  </view>
</view>

<!--确定按钮-->
<button type="primary" style="margin:59rpx 30rpx 0 30rpx;" bindtap="nato_cash5">确定</button>
<view style="margin-top:24rpx;display:flex;justify-content:center;">
  <text style="font-size:26rpx;line-height:36rpx;letter-spacing:-0.6rpx;color:rgb(142,142,142)">每周二审核并打款。到账时间：1-4个工作日到账。
  </text>
</view>
<view style="margin-top:29rpx;display:flex;justify-content:center;" bindtap="nato_cash6">
  <text style="font-size:26rpx;line-height:36rpx;letter-spacing:-0.6rpx;color:rgb(61,209,164)">提现记录
  </text>
</view>
<view style="height:51rpx;"></view>

<!--拍照上传弹框-->
<view class="check-succeed" style="{{applySucceed}}">
  <view class="check-succeed-toast">
    <image class="check-succeed-icon" mode="widthFix" src="https://cdn.51julebu.com/xiaochengxu/image/x@2x.png" bindtap="closeCheck"></image>
    <view class="check-succeed-title">示例</view>
    <view class="check-succeed-content">请上传清晰的手持身份证合照</view>
    <image class="check-succeed-img" mode="widthFix" src="https://cdn.51julebu.com/xiaochengxu/image/sfrzzp@2x.png"></image>
    <view style="position:relative">
      <view class="check-succeed-btn" bindtap="toast_btn_upload">上传</view>
      <view class="check-succeed-line"></view>
      <view class="check-succeed-btn" bindtap="toast_btn_camera">拍照</view>
    </view>
  </view>
</view>