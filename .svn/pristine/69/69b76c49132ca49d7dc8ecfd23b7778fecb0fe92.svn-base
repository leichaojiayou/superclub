/*手机号码弹框*/

.phone {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 99;
  opacity: 0;
  transition: opacity 400ms ease-in;
  pointer-events: none;
  display: flex;
  justify-content: center;
  align-items: center;
}

.phone-dialog {
  position: fixed;
  top: 20%;
  width: 84%;
  background-color: #fff;
  border-radius: 6px;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.phone-text {
  margin: 33rpx 40rpx 31rpx;
  font-size: 36rpx;
  text-align: center;
}

.phone-box {
  position: relative;
  margin: 0 40rpx 25rpx;
}

.phone-input {
  display: inline-block;
  background: #fff;
  border-radius: 4px;
  border: 1px solid #e0e0e0;
  height: 70rpx;
  width: 100%;
}

.phone-button {
  text-align: center;
  width: 100%;
  margin-bottom: 30rpx;
}

.phone-btn {
  display: inline-block;
  padding: 0 90rpx;
  border-radius: 4px;
  margin: 0 10px;
}

.phone-cancel {
  color: #8e8e8e;
  background: #f5f5f5;
}

.phone-canadd {
  border: 1px solid #e5e7ed;
  background: rgb(61, 209, 165);
  color: #fff;
}

/*sdfsf*/

page {
  background-color: rgb(255, 255, 255);
  white-space: normal;
  overflow: scroll;
}

.header {
  padding: 87rpx 0 70rpx 0;
  justify-content: space-between;
}

.header, .meta_item, .body, .body_item, .foot {
  display: flex;
  background-color: rgb(255, 255, 255);
}

.meta_item {
  width: 25%;
  flex-direction: column;
  align-items: center;
}

.sort_num {
  margin-top: 30rpx;
  font-size: 38rpx;
  letter-spacing: -1rpx;
  width: 120rpx;
  overflow: hidden;
  text-align: center;
}

/*body样式*/

.body {
  padding-top: 69rpx;
  flex-wrap: wrap;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.body_item {
  flex-direction: column;
  align-items: center;
  width: 33.3%;
}

/*foot*/

.foot {
  position: relative;
  flex-wrap: wrap;
  margin-top: 2rpx;
  padding: 63rpx 161rpx 0 161rpx;
  justify-content: space-around;
  border-top: 1rpx solid rgb(230, 230, 230);
  border-bottom: 86rpx solid white;
}

.share {
  width: 559rpx;
  position: absolute;
  top: -14.5rpx;
  left: 12.7%;
  height: 29rpx;
  font-size: 26rpx;
  letter-spacing: -0.6rpx;
  color: rgb(142, 142, 142);
  white-space: nowrap;
}

.list_img1 {
  width: 78rpx;
  height: 78rpx;
  opacity: 30%;
  padding: 10rpx;
}

.point {
  background: red;
  width: 40rpx;
  height: 40rpx;
  border-radius: 50px;
  position: absolute;
  top: 52rpx;
  right: 80rpx;
  text-align: center;
  line-height: 40rpx;
}

.body_item1 {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 33.3%;
  position: relative;
}

.manage-footer {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  padding: 30rpx 30rpx 50rpx;
  border-bottom: 1rpx solid #e6e6e6;
  background: #fff;
}

.manage-footer-text {
  display: flex;
  justify-content: center;
  align-items: center;
  color: #8e8e8e;
  font-size: 30rpx;
  padding-bottom: 40rpx;
}

.manage-footer-img {
  margin: 0 auto;
  width: 60%;
