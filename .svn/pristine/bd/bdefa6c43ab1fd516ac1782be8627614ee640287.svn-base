


  <!--pages/cash7/cash7.wxml-->
  <view style="height:31rpx;"></view>

  <!--header-->
  <view class="header">
    <text class="t32rpx">提现</text>
    <text class="t38rpx">160.00</text>
  </view>

  <!--body-->
  <view class="body" wx:for="{{item}}">

    <!--提现至-->
    <view class="body_item">
      <text class="t28rpx">提现至</text>
      <text class="t28rpx">{{item.bank}}</text>
    </view>

    <!--提现账户-->
    <view class="body_item">
      <text class="t28rpx">提现账户</text>
      <text class="t28rpx">{{item.num}}</text>
    </view>

    <!--创建时间-->
    <view class="body_item">
      <text class="t28rpx">创建时间</text>
      <text class="t28rpx">{{item.time}}</text>
    </view>

    <!--状态-->
    <view class="body_item">
      <text class="t28rpx">状态</text>
      <text class="t28rpx" style="color:{{item.color}}">{{item.status}}</text>
    </view>

    <!--e-->
  </view>



<!--foot-->
<view class="foot" wx:for="{{item1}}">

  <!--绿色线条-->
  <view class="greenpoint"></view>
  <view class="greenline"></view>

  <!--右边框-->
  <view class="foot_item">
  <view>
<text class="t28rpx">{{item.shenqing}}</text>
</view>
<text class="t22rpx">{{item.time}}</text>
<icon type="{{item.icontype}}" class="icon" iconSize="15" ></icon>

<!--文字-->
<view style="margin-top:77rpx;">
<text class="t28rpx">{{item.shenhe}}</text>
</view>
<text class="t22rpx">{{item.updatatime}}</text>

  </view>



  <!--e-->
</view>