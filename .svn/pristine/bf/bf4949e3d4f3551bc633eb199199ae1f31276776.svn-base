/*活动二维码*/
.activity-code-title {
  text-align: center;
  margin: 20rpx 0 10rpx;
  padding: 0 10% 20rpx;
  font-size: 32rpx;
}

.activity-code-text {
  text-align: center;
  font-size: 24rpx;
}

.activity-code-img {
  width: 60%;
  margin: 40rpx auto 20rpx;
}

.activity-code-btn {
  width: 80%;
  background: #3dd1a5;
  color: #fff;
  margin-bottom: 30rpx;
}

.activity-code-text {
  opacity: 0;
  position: fixed;
  left: 0;
  right: 0;
  top: 68%;
  z-index: 99;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.activity-code-hehe {
  text-align: center;
  color: #fff;
}

.activity-code-share {
  width: 60%;
  text-align: center;
  margin: 20rpx auto 30rpx;
  color: #3dd1a5;
  background: rgb(61, 209, 165, 0);
  border: 1px solid #3dd1a5;
  border-radius: 10rpx;
  padding: 10rpx 0;
}

.activity-code-close  {
  width: 60rpx;
  margin: 0 auto;
  text-align: center;
}

/*发送验证码弹框*/
.modal {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 99;
  opacity: 0;
  transition: opacity 400ms ease-in;
  pointer-events: none;
  /*display: flex;
  justify-content: center;
  align-items: center;*/
}

.modal .dialog {
  position: fixed;
  top: 20%;
  left: 50%;
  margin-left: -42%;
  width: 84%;
  background-color: #fff;
  border-radius: 6px;
  display: flex;
  flex-direction: column;
  /*justify-content: space-between;*/
}

.modol-text {
  margin: 33rpx 40rpx 31rpx;
  font-size: 36rpx;
}

.modal-box {
  position: relative;
  margin: 0 40rpx 25rpx;
}

.model-input {
  display: inline-block;
  background: #fff;
  border-radius: 4px;
  border: 1px solid #e0e0e0;
  height: 70rpx;
  width: 60%;
  padding-left: 28rpx;
}

.modal-time {
  position: absolute;
  top: 18rpx;
  right: 10rpx;
  font-size: 32rpx;
  color: #8e8e8e;
  z-index: 100;
}

.modal-button {
  text-align: center;
  width: 100%;
  margin-bottom: 28rpx;
}

.modal-btn {
  display: inline-block;
  padding: 0 80rpx;
  border-radius: 4px;
  margin: 0 10px;
}

.modal-cancel {
  color: #8e8e8e;
  background: #f5f5f5;
}

.modal-canadd {
  border: 1px solid #e5e7ed;
  background: rgb(61, 209, 165);
  color: #fff;
}
.modal-box-tip{
  text-align: center;
  color:#8e8e8e;
  font-size: 26rpx;
  margin-top: 10rpx;
}

