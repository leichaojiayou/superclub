.code-message {
  margin: 30rpx auto 0;
}
.code-message-align{
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}
.code-message-logo {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}

.code-message-icon {
  position: absolute;
