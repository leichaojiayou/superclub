
<!--pages/duanxintixing/duanxintixing.wxml-->
<view style="height:31rpx;border-bottom:1rpx solid rgb(230,230,230)"></view>

<!--时间循环-->
<view style="background-color: rgb(255, 255, 255);" bindtap="changehidden">
  <view class="item" wx:for="{{item}}"  id="{{item.id}}">
    <text>{{item.time}}</text>

    <!--if判断-->
    <view wx:if="{{hidden==item.id}}">
      <icon type="{{item.icontype}}" size="18"  ></icon>
    </view>

  </view>
</view>
<view class="foot">
  <text class="t26rpx">我们将会在你选定时间给所有报名的用户发送短信，提醒活动即将开始。</text>
</view>