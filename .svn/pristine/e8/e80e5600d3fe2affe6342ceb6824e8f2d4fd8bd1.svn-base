/*顶部*/

.top_view {
  background-color: rgb(255, 255, 255);
  width: 100%;
  height: 88rpx;
  display: flex;
  align-items: center;
}

/*搜索view*/

/*状态2  点击搜索后*/

.search_view {
  width: 590rpx;
  height: 60rpx;
  margin: 14rpx 0 14rpx 30rpx;
  background-color: rgb(240, 240, 240);
  display: flex;
  align-items: center;
  border-radius: 10rpx;
}

.search_view input {
  margin-left: 26rpx;
  color: rgb(26, 26, 26);
  font-size: 32rpx;
}

.search_view_text {
  font-size: 32rpx;
  margin-left: 36rpx;
  color: rgb(61, 209, 164);
}

/*状态1*/

.search_view1 {
  width: 690rpx;
  height: 60rpx;
  margin: 14rpx 30rpx 14rpx 30rpx;
  background-color: rgb(240, 240, 240);
  display: flex;
  flex-direction: column;
  text-align: center;
  border-radius: 10rpx;
}

.search_view1_icon {
  color: rgb(142, 142, 142);
  font-size: 28rpx;
  display: flex;
  align-items: center;
  margin-left: 299rpx;
  margin-top: 16rpx;
}

.icon {
  opacity: 0.3;
}

/*上下变线*/

.border {
  border-bottom: 1rpx solid rgb(230, 230, 230);
}

.border1 {
  border-top: 1rpx solid rgb(230, 230, 230);
  border-bottom: 1rpx solid rgb(230, 230, 230);
}

/*list循环  状态1*/

.mid_item_view {
  background-color: rgb(246, 246, 246);
  height: 88rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

/*分组按钮框*/

.mid_item_mid_view {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
}

.icon_view {
  width: 50rpx;
}

/*组按钮*/

.css_icon_true {
  border-right: 15rpx solid transparent;
  border-top: 20rpx solid rgba(26, 26, 26, 0.5);
  border-left: 15rpx solid transparent;
}

.css_icon_false {
  border-top: 15rpx solid transparent;
  border-bottom: 15rpx solid transparent;
  border-left: 20rpx solid rgba(26, 26, 26, 0.5);
}

.text_groud_view {
  display: flex;
  justify-content: center;
  align-items: center;
}

/*中字体*/

.mid_text {
  font-size: 32rpx;
  margin-left: 11rpx;
  color: rgb(26, 26, 26);
}

/*小字体*/

.small_text {
  color: rgb(142, 142, 142);
  font-size: 24rpx;
  margin-left: 16rpx;
}

/*list*/

/*等级图标*/

.user_icon {
  width: 36rpx;
  height: 36rpx;
}

/*文字圆图标*/

/*会长 副会长*/

.text_icon {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 76rpx;
  height: 28rpx;
  font-size: 20rpx;
  margin-left: 13rpx;
  text-align: center;
  border-radius: 4rpx;
  line-height: 36rpx;
  color: rgb(246, 200, 121);
  border: 1rpx solid rgb(246, 200, 121);
}

/*管理员*/

.text_icon1 {
  display: flex;
  align-items: center;
  justify-content: center;
  line-height: 36rpx;
  width: 76rpx;
  height: 28rpx;
  font-size: 20rpx;
  margin-left: 13rpx;
  text-align: center;
  border-radius: 4rpx;
  color: rgb(162, 158, 195);
  border: 1rpx solid rgb(162, 158, 195);
}

.list_view1 {
  display: flex;
  flex-shrink: 0;
  align-items: center;
  background-color: rgb(255, 255, 255);
  height: 138rpx;
}

.user_image {
  border-radius: 10rpx;
  margin: 29rpx 0 29rpx 29rpx;
  width: 80rpx;
  height: 80rpx;
}

.user_info_top {
  width: 82%;
  height: 100%;
  margin-left: 24rpx;
  display: flex;
  align-items: center;
  border-top: 1rpx solid rgb(230, 230, 230);
}

/*状态3*/

.statu3_view {
  text-align: center;
  margin-top: 310rpx;
  font-size: 48rpx;
  color: rgb(142, 142, 142);
}
