<!--pages/cash/cash.wxml-->

<view class="line1"  bindtap="nato_cash3_1">
<!--设置提现用户-->
<text style="font-size:32rpx;line-height:36rpx;letter-spacing:-0.8rpx;">{{line1}}</text>
<image src="../../../assets/images/right-errow.png" class="line1_img"></image>

</view>



<view class="line2">
<!--金额-->
<text style="font-size:32rpx;line-height:36rpx;letter-spacing:-0.8rpx;">{{line2}}</text>
<text style="font-size:32rpx;line-height:36rpx;letter-spacing:-0.8rpx;color:rgb(191,191,191)">可提现金额{{line2money}}</text>
</view>
 <!--每天一次，单笔~-->
<view class="line2_bottom">
<text style="font-size:26rpx;line-height:36rpx;letter-spacing:-0.6rpx;color:rgb(142,142,142)">每天一次，单笔最高1万元。</text>
<text style="font-size:26rpx;line-height:36rpx;letter-spacing:-0.8rpx;color:rgb(61,209,164)">提现规则</text>
</view>

<view class="line3">
<!--真实姓名-->
<text style="font-size:32rpx;line-height:36rpx;letter-spacing:-0.8rpx;">{{realname}}</text>
<input placeholder="输入" class="input" placeholder-style="font-size:32rpx;line-height:36rpx;letter-spacing:-0.8rpx;color:rgb(191,191,191)" maxlength="10"/>
</view>




<!--身份证正面~手持本人身份证拍照-->


<view class="body4">

<!--身份证正面-->
<view class="body4_1">
<text style="font-size:32rpx;line-height:36rpx;letter-spacing:-0.8rpx;">身份证正面</text>
<image src="../../../assets/images/club_logo.jpg" class="body4_1img" mode="aspectFill"></image>
<image src="../../../assets/images/right-errow.png" class="line1_img1"></image>
</view>
<!--身份证反面-->

<view class="body4_2">
<text style="font-size:32rpx;line-height:36rpx;letter-spacing:-0.8rpx;">身份证正面</text>
<image src="../../../assets/images/club_logo.jpg" class="body4_1img" mode="aspectFill"></image>
<image src="../../../assets/images/right-errow.png" class="line1_img1"></image>
</view>
<!--手持本人身份证拍照-->
<view class="body4_3">
<text style="font-size:32rpx;line-height:36rpx;letter-spacing:-0.8rpx;">手持本人身份证拍照</text>
<image src="../../../assets/images/right-errow.png" class="line1_img1"></image>
</view>
<!--e-->
</view>



<!--身份认证后不可修改，审核通过后即可提现。-->
<view style="margin:16rpx 0 0 30rpx;">
<text style="font-size:26rpx;line-height:36rpx;letter-spacing:-0.6rpx;color:rgb(142,142,142)">身份认证后不可修改，审核通过后即可提现。</text>
</view>

<!--确定按钮-->
<button type="primary" style="margin:59rpx 30rpx 0 30rpx;" bindtap="nato_cash5">确定</button>

<view style="margin-top:24rpx;display:flex;justify-content:center;">
<text style="font-size:26rpx;line-height:36rpx;letter-spacing:-0.6rpx;color:rgb(142,142,142)">每周二审核并打款。到账时间：1-4个工作日到账。
</text>
</view>


<view style="margin-top:29rpx;display:flex;justify-content:center;" bindtap="nato_cash6">
<text style="font-size:26rpx;line-height:36rpx;letter-spacing:-0.6rpx;color:rgb(61,209,164)">提现记录
</text>
</view>
<view style="height:51rpx;"></view>