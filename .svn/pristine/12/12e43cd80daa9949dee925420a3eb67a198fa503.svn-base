@import "../../../template/common/common.wxss";
@import "../../../template/toasts/toasts.wxss";
@import "../../club/clubhome/clubhome.wxss";
page{
  padding-bottom: 100rpx;
}
.back_head {
  background-color: rgb(89, 215, 176);
}

.back_top {
  display: flex;
  justify-content: space-between;
  align-items: center;
}


.club_msg {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin-left: 30rpx;
  padding-bottom: 22rpx;
}


.back_foot {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  position: relative;
  padding: 40rpx 0 60rpx 0;
}


.label_txt {
  color: rgb(58, 157, 126);
  font-size: 19px;
  
}

.xiegan {
  position: absolute;
  right: 66.6%;
  top: 15%;
  width: 14.8rpx;
  height: 24.8rpx;
}

.xiegan2 {
  position: absolute;
  right: 33.3%;
  top: 15%;
  width: 14.8rpx;
  height: 24.8rpx;
}

.point {
  background: red;
  font-size: 26rpx;
  color: #fff;
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  position: absolute;
  top: 0;
  right: 0;
  text-align: center;
  line-height: 40rpx;
}

.body_item1 {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}

.list_img {
  width: 78rpx;
  height: 78rpx;
  opacity: 30%;
}
.nd_img_class{
  width: 112px;
  height: 77px;
}
.common2 {
  border-bottom: 1px solid rgb(246, 246, 246);
  position: relative;
  margin-bottom: 0;
  padding-bottom:0;
}
.freemsg-position{
  position: absolute;
  right: 0;
  bottom: 0;
}
.nd_header{
  margin: 0;
  padding: 0 20rpx;
}
.mangage-money{
  position: absolute;
  right: 20rpx;
  bottom: 0;
}
.manage-list-bg{
  background: #fff;
}
.manage-club-logo{
  flex-shrink: 0;
  width: 140rpx;
  height: 140rpx;
  border-radius: 10rpx;
  margin: 0 20rpx 0 37rpx;
  background: none;
}
.manage-club-content{
  display: flex;
  justify-content: center;
  margin: 29rpx 0 38rpx 0;
  align-items: center;
  color: #fff;
}
.manage-club-errow{
  width: 36rpx;
  height: 38rpx;
  padding-right: 20rpx;
}
.manage-club-text{
  padding-top: 34rpx;
  font-size: 28rpx;
  display: block;
}


.midview_list{
  padding-bottom: 20rpx;
}
.border_1 {
  font-size: 24rpx;
  color: rgb(142, 142, 142);
  display: flex;
  justify-content: center;
  align-items: center;
  background-color:white;
}

.border_1_1 {
  border-bottom: 1rpx solid rgb(230, 230, 230);
  width: 130rpx;
}

.club-index-title{
  display:flex;
  width:400rpx;
}

.txt-overflow{
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}