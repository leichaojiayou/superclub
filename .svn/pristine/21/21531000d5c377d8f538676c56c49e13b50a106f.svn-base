/*<!--钱topview-->*/

.top_view {
  background-color: rgb(89, 215, 176);
  color: rgb(255, 255, 255);
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 400rpx;
  font-size: 32rpx;
}

.money_text {
  font-size: 98rpx;
}

/*-----------*/

/*中间view*/

.mid_view {
  font-size: 34rpx;
  border-top: 1rpx solid rgb(230, 230, 230);
  border-bottom: 1rpx solid rgb(230, 230, 230);
  margin-top: 30rpx;
  background-color: rgb(255, 255, 255);
  height: 528rpx;
}

.item_view {
  height: 132rpx;
  display: flex;
  align-items: center;
}

.item_mid_view {
  height: 100%;
  margin-left: 30rpx;
  display: flex;
  align-items: center;
  width: 614rpx;
  justify-content: space-between;
  border-bottom: 1rpx solid rgb(230, 230, 230);
}

.item_mid_view1 {
  height: 131rpx;
  margin-left: 30rpx;
  display: flex;
  align-items: center;
  width: 614rpx;
  justify-content: space-between;
  border-top: 1rpx solid rgb(230, 230, 230);
  border-bottom: 1rpx solid rgb(230, 230, 230);
}

/*图标 图片*/

.img {
  width: 72rpx;
  height: 72rpx;
  padding-left: 34rpx;
}

.icon_img {
  padding-right: 30rpx;
  width: 18rpx;
  height: 31rpx;
  opacity: 0.8;
}

.text_icon {
  margin-right: 31rpx;
  text-align: center;
  font-size: 25rpx;
  line-height: 36rpx;
  width: 36rpx;
  height: 36rpx;
  color: white;
  background-color: red;
  border-radius: 50%;
}

.text_icon_view {
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.foot_text {
  padding-right: 29rpx;
  padding-left: 11rpx;
  font-size: 26rpx;
  color: rgb(142, 142, 142);
}
.identity-icon{
  width: 28rpx;
  height: 28rpx;
}