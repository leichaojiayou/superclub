
<!--pages/myaccount/myaccount.wxml-->
<image src="{{header_img}}" class="msg_img head_img"></image>
<view class="msg_name head_name">{{name}}</view>
<view wx:if="{{status==0}}" class="foot">
  <view class="msg_num des_txt">如您已注册超级俱乐部账号，请绑定账号。</view>
  <button type="primary" class="btn" bindtap="nato_bindphone">绑定账号</button>
</view>
<view wx:if="{{status==1}}" class="foot">
  <view class="msg_num des_txt">已绑定超级俱乐部账号：{{idnumber}}</view>
</view>