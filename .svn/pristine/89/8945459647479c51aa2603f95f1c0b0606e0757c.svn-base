import api from '../request.js'



/**
 * 小程序登陆: http://doc.iweju.com/FitNesse.WejuBaHttp.BindDoc.UserWxLoginDoc
 * 请求参数： code， encryptedData，iv
 */
const login = (param) => api.wxRequest(param, "/ba/user/wxlogin")


/**
 * 查看自己参与的俱乐部列表: http://doc.iweju.com/FitNesse.WejuBaHttp.ClubDoc.ClubHomeListDoc
 */
const joinedClubs = (param) => api.wxRequest(param, "/ba/club/home/list")

/**
 * 微信绑定账号的检查: 
 */
const phoneCheck = (param) => api.wxRequest(param, "/ba/account/phone/login/check")


module.exports = {
    login,
    joinedClubs,
    phoneCheck
}