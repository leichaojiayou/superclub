<template is="helplist" data="{{item}}" />
<template is="helplist" data="{{item}}" />
<view class="batten">
  <button class="botbtn" bindtap="nato_payment">马上报名</button>
</view>
<view class="batten">
  <button bindtap="nato_apply_page">帮人报名</button>
</view>
<template name="helplist">
  <view class="container" style="position:relative ">
    <view class="batten" style="padding-top:17px;">
      <text class="txtmargin">昵称</text>
      <text>{{item.name}}</text>
    </view>
    <view class="batten">
      <text class="txtmargin">性别</text>
      <text>{{item.sex}}</text>
    </view>
    <view class="batten">
      <text class="txtmargin">手机</text>
      <text>{{item.phone}}</text>
    </view>
    <view class="batten">
      <text class="txtmargin">保险</text>
      <text>{{item.baoxian}}</text>
    </view>
    <view class="btn des_txt">
      <text class="txtmargin" bindtap="nato_apply_page">编辑</text>
      <text>删除</text>
    </view>
  </view>
</template>