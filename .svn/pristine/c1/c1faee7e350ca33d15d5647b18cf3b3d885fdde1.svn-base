page {
  white-space: normal;
}

.header {
  width: 750rpx;
  height: 266rpx;
  background: #fff;
  border-bottom: 1rpx solid rgb(230, 230, 230);
  border-top: 1rpx solid rgb(230, 230, 230);
  shape-outside: 1rpx;
}

.header_title {
  margin: 38rpx 210rpx 0rpx 29rpx;
  font-size: 32rpx;
  line-height: 50rpx;
  letter-spacing: -0.8rpx;
}

.header_time {
  margin-left: 30rpx;
  font-size: 24rpx;
  line-height: 36rpx;
  letter-spacing: -0.6rpx;
  color: rgb(142, 142, 142);
}

.header_msg {
  margin-top: 20rpx;
  display: flex;
}

.header_btn {
  font-size: 22rpx;
  color: rgb(61, 209, 164);
  height: 40rpx;
  width: 135rpx;
  line-height: 40rpx;
  text-align: center;
  border: 1rpx;
  border-style: solid;
  border-color: rgb(61, 209, 164);
  border-radius: 8rpx;
  margin-right: 10rpx;
}

.header_top {
  display: flex;
}

.isno {
  margin-top: 48rpx;
  font-size: 18rpx;
  line-height: 36.8rpx;
  letter-spacing: -0.4rpx;
  color: rgb(142, 142, 142);
  width: 180rpx;
}

.money {
  color: rgb(61, 209, 165);
  font-size: 32rpx;
  letter-spacing: -0.8rpx;
  line-height: 56.6rpx;
}

.item2_title {
  font-size: 32rpx;
  line-height: 50rpx;
  letter-spacing: -0.8rpx;
}

.item2_top {
  display: flex;
  justify-content: space-between;
}

.item2 {
  height: 115rpx;
  padding: 39rpx 31rpx 0rpx 29rpx;
  width: 690rpx;
  background: #fff;
  border-top: 1rpx solid rgb(230, 230, 230);
  shape-outside: 1rpx;
}

.item2_bottom {
  display: flex;
  justify-content: space-between;
}

.header_time2 {
  font-size: 24rpx;
  line-height: 36rpx;
  letter-spacing: -0.6rpx;
  color: rgb(142, 142, 142);
}

.pay {
  font-size: 18rpx;
  line-height: 36.8rpx;
  letter-spacing: -0.4rpx;
  color: rgb(142, 142, 142);
}
.no_image{
  margin-top: 360rpx;
  width: 100rpx;
  height: 100rpx;

}
.item_wu{
  width: 750rpx;
  text-align: center;
}
.wu_text{
  
  font-size: 26rpx;
  letter-spacing: -0.6rpx;
  color: rgb(142, 142, 142);
}
