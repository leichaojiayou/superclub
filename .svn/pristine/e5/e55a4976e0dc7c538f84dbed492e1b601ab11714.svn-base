/*header*/
.header{
    height: 134rpx;
    background-color: rgb(255, 255, 255);
    padding: 0 34rpx 0 29rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
/*body*/
.body{
    margin: 20rpx 0 21rpx 0;
      padding: 25rpx 34rpx 25rpx 29rpx;
    height: 260rpx;
    background-color: rgb(255, 255, 255);
}
.body_item{
    display: flex;
    align-items: center;
    justify-content: space-between;
    height: 25%;
}
/*foot*/
.foot{
    height: 300rpx;
    background-color: rgb(255, 255, 255);
    padding: 44rpx 0 48rpx 120rpx;
    display: flex;
    flex-direction: row;
    position: relative;
}
/*绿色线条*/
.greenline{
    position: absolute;
    height: 160rpx;
    left: 74rpx;
    top: 77rpx;
    border-right: 1rpx solid rgb(61,209,165);

    
}
.greenpoint{
    position:absolute;
    left: 69rpx;
    top:68rpx;
    height: 12rpx;
    width: 12rpx;
    border-radius: 6rpx;
    background-color: rgb(61,209,165);
}
.icon{
    position: absolute;
    left: 59rpx;
    top: 224rpx;
    width: 32rpx;
    height: 32rpx;
}