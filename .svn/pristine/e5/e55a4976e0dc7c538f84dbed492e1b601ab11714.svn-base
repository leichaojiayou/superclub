/* pages/cash7/cash7.wxss */
.t32rpx{
    font-size: 32rpx;
    line-height: 44rpx;
    letter-spacing: -0.8rpx;
}
.t38rpx{
    font-size: 38rpx;
    line-height: 56.6rpx;
    letter-spacing: -1rpx;
}

.t28rpx{
    font-size: 28rpx;
    line-height: 36rpx;
    letter-spacing: -1rpx;
    color: rgb(142, 142, 142);
}
.t22rpx{
    font-size: 22rpx;
