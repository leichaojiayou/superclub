<view class="header">
  <!--俱乐部logo-->
  <text class="header_txt">俱乐部Logo</text>
  <!--俱乐部图片-->
  <view class="header_img">
    <image src="../../../assets/images/club.png" class="header_img1"></image>
  </view>
  <image src="../../../assets/images/right-errow.png" class="right_errow" mode="aspectFill"></image>
</view>


<!--名称-->
<view style=" background-color: rgb(255, 255, 255);">
  <view class="name_item">
    <view style="margin-top: 29rpx;margin-bottom:15.5pt;">
      <text class="name_txt ">名称</text>
      <text class="name_txt1">俱乐部名称</text>
    </view>
    <image src="../../../assets/images/right-errow.png" class="right_errow" mode="aspectFill"></image>
  </view>
</view>


<!--所在地-->
<view style=" background-color: rgb(255, 255, 255);">
  <view class="name_item">
    <view style="margin-top: 14.5pt;margin-bottom:15.5pt;">
      <text class="name_txt ">所在地</text>
      <text class="choose_txt">{{choose}}</text>
    </view>
    <image src="../../../assets/images/right-errow.png" class="right_errow" mode="aspectFill"></image>
  </view>
</view>


<!--成立时间-->
<view style=" background-color: rgb(255, 255, 255);">
  <view class="name_item">
    <view style="margin-top: 14.5pt;margin-bottom:15.5pt;">
      <text class="name_txt ">成立时间</text>
      <text class="choose_txt">{{choose}}</text>
    </view>
    <image src="../../../assets/images/right-errow.png" class="right_errow" mode="aspectFill"></image>
  </view>
</view>


<!--主打项目-->
<view style=" background-color: rgb(255, 255, 255);">
  <view class="name_item">
    <view style="margin-top: 14.5pt;margin-bottom:15.5pt;">
      <text class="name_txt ">主打项目</text>
      <text class="choose_txt">{{choose}}</text>
    </view>
    <image src="../../../assets/images/right-errow.png" class="right_errow" mode="aspectFill"></image>
  </view>
</view>


<!--宣言-->
<view style=" background-color: rgb(255, 255, 255);">
  <view class="name_item">
    <view style="margin-top: 14.5pt;margin-bottom:15.5pt;">
      <text class="name_txt ">宣言</text>
      <text class="choose_txt">{{input}}</text>
    </view>
    <image src="../../../assets/images/right-errow.png" class="right_errow" mode="aspectFill"></image>
  </view>
</view>


<!--简介-->
<view style=" background-color: rgb(255, 255, 255);border-bottom: 0.5pt solid rgb(229, 229, 229);">
  <view class="name_item">
    <view style="margin-top: 14.5pt;margin-bottom:15.5pt;">
      <text class="name_txt ">简介</text>
      <text class="choose_txt">{{txt}}</text>
    </view>
    <image src="../../../assets/images/right-errow.png" class="right_errow" mode="aspectFill"></image>
  </view>
</view>



<!--负责人-->
<view class="host">
<text class="host_txt">负责人</text>
<input placeholder="输入" class="input_txt"/>

</view>



<!--联系电话-->
<view class="phonecontact">
  <view class="name_item">
    <view style="margin-top: 29rpx;margin-bottom:15.5pt;">
      <text class="name_txt ">联系电话</text>
      <input placeholder="输入" class="input_txt1" type="number"/>
    </view>
  </view>
</view>


<button type="primary" class="btn">提交审核</button>
<view style="height:85rpx;  background-color: rgb(246, 246, 246);"></view>

