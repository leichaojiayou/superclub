.header {
  width: 750rpx;
  background: #fff;
  border: 1rpx;
  border-style: solid;
  border-color: rgb(229, 229, 229);
  shape-outside: 1rpx;
}

.header_title {
  margin: 38rpx 210rpx 0rpx 29rpx;
  font-size: 32rpx;
  line-height: 50rpx;
  letter-spacing: -0.8rpx;
}

.header_time {
  margin-left: 30rpx;
  font-size: 24rpx;
  line-height: 36rpx;
  letter-spacing: -0.6rpx;
  color: rgb(142, 142, 142);
}

.header_msg {
  margin-top: 20rpx;
  display: flex;
}

.header_btn {
  font-size: 22rpx;
  color: rgb(61, 209, 164);
  height: 40rpx;
  width: 135rpx;
  line-height: 40rpx;
  text-align: center;
  border: 1rpx;
  border-style: solid;
  border-color: rgb(61, 209, 164);
  border-radius: 8rpx;
  margin-right: 10rpx;
}

.center {
  margin-bottom: 71rpx;
  margin-top: 49rpx;
  text-align: center;
  font-size: 40rpx;
  line-height: 36rpx;
  letter-spacing: -1rpx;
  color: rgb(26, 26, 26);
}

.list {
  line-height: 84rpx;
  width: 750rpx;
  height: 84rpx;
  display: flex;
}

.triangle {
  margin-top: 28rpx;
  margin-left: 21rpx;
  width: 30rpx;
}

.list_text {
  font-size: 28rpx;
  letter-spacing: -0.8rpx;
  color: rgb(26, 26, 26);
  padding-left: 23rpx;
}

.css_icon_true {
  border-right: 15rpx solid transparent;
  border-top: 20rpx solid rgba(26, 26, 26, 0.5);
  border-left: 15rpx solid transparent;
}

.css_icon_false {
  border-top: 15rpx solid transparent;
  border-bottom: 15rpx solid transparent;
  border-left: 20rpx solid rgba(26, 26, 26, 0.5);
}

.item {
  display: flex;
  background: #fff;
  width: 750rpx;
  height: 128rpx;
  border: 1rpx;
  border-style: solid;
  border-color: rgb(230, 230, 230);
}

.item_point {
  margin: 30rpx 0rpx 0rpx 21rpx;
  width: 40rpx;
  height: 40rpx;
  border-radius: 50rpx;
  background: rgb(157, 157, 157);
}

.item_name {
  font-size: 32rpx;
  line-height: 36rpx;
  letter-spacing: -0.8rpx;
  color: rgb(26, 26, 26);
}

.item_time {
  font-size: 22rpx;
  line-height: 36rpx;
  letter-spacing: -0.6rpx;
  color: rgb(191, 191, 191);
}

.item_top {
  width: 636rpx;
  display: flex;
  justify-content: space-between;
}

.item_name_text {
  color: rgb(61, 209, 164);
  font-size: 32rpx;
  letter-spacing: -0.8rpx;
}

.no {
  width: 750rpx;
  height: 940rpx;
  text-align: center;
  line-height: 940rpx;
  font-size: 26rpx;
  letter-spacing: -0.6rpx;
  color: rgb(142, 142, 142);
}

/*头像图标*/

.item_img {
  margin: 30rpx 0rpx 0rpx 21rpx;
  width: 40rpx;
  height: 40rpx;
  border-radius: 50rpx;
}
