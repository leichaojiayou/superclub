
page {
  word-wrap: break-word;
  white-space: normal;
  background-color: rgb(255, 255, 255);
}
.t34rpx{
    display: flex;
    justify-content: space-around;
    align-items: center;
    font-size: 34rpx;
    letter-spacing: -0.8rpx;
}
.body{
    margin-top: 50rpx;

}

.txt_position{
    display: flex;
    justify-content: center;
    height: 50rpx;
    margin-bottom: 20rpx;
    background-color: rgb(255, 255, 255);
}
/*图片循环样式*/

.body_item{
    flex-wrap: wrap;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 22rpx;
}
.img{
    height: 108rpx;
    width: 230.4rpx;
}
.tex-line{
    width: 100rpx;
    height: 2rpx;
    background: rgb(230, 230, 230)
}
/*图片预览*/
.swiper-container {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: #000;
}
.swiper-container image {
  width: 100%;
  height: 100%;
}
