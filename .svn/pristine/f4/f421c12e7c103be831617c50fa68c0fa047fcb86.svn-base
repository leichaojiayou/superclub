@import "../../../template/common/common.wxss";
@import "../../../template/toasts/toasts.wxss";

.manage {
  display: flex;
  flex-direction: row;
  background-color: yellow;
  justify-content: space-around;
}

.container1 {
  height: 177rpx;
  background-color: white;
  border-bottom: 2.2vh solid rgb(246, 246, 246);
}

.mycharge {
  display: flex;
  align-items: center;
  padding: 40rpx 0 16rpx 32rpx;
}

.mycharge1 {
  padding: 0 0 36rpx 32rpx;
}

.mycharge_txt {
  font-size: 34rpx;
  letter-spacing: -0.8rpx;
}

/*.des_txt {
  padding-left: 30rpx;
}*/

/* pages/index_empty/index_empty.wxss */

page {
  white-space: normal;
}

.header {
  /*height: 208rpx;*/
  background-color: rgb(255, 255, 255);
  /*margin-top: 33rpx;*/
  display: flex;
  align-items: flex-start;
  position: relative;
}

.header_item {
  margin-left: 32rpx;
}

.header_img {
  height: 130rpx;
  width: 130rpx;
  border-radius: 10rpx;
  /*border: 1rpx solid rgb(180, 180, 180);
  margin-left: 32rpx;*/
}

.arrowright {
  width: 18rpx;
  height: 31rpx;
  opacity: 0.2;
  position: absolute;
  right: 29rpx;
}

/*俱乐部名字*/

.club_id {
  width: 400rpx;
  font-size: 34rpx;
  letter-spacing: -0.8rpx;
  display:  -webkit-box;
  -webkit-line-clamp:  1;
  -webkit-box-orient:  vertical;
  padding-right: 30rpx;
}

/*中间分类*/

.sort {
  flex-direction: row;
  color: #fff;
  margin-top: 0;
}

/*每一类的样式*/

.sort_item {
  background-color: red;
  /*padding: 4.5px 8.5px;*/
  margin-right: 3%;
  border-radius: 3px;
}

/*有几个活动样式*/

.nums {
  letter-spacing: -0.3px;
  color: rgb(142, 142, 142);
  margin-top: 0;
}

/*body*/

.body_img {
  height: 115rpx;
  width: 141rpx;
}

.body_txt {
  font-size: 28rpx;
  line-height: 42rpx;
  letter-spacing: -0.6rpx;
  color: rgb(142, 142, 142);
}

.fun_line {
  border-bottom: 1rpx solid rgb(230, 230, 230);
  height: 46.5rpx;
  margin: 0 23.6% 0 23.6%;
}

.fun_txt {
  font-size: 24rpx;
  letter-spacing: -0.6rpx;
  color: rgb(142, 142, 142);
  position: absolute;
  top: 33%;
  left: 43%;
  background-color: rgb(255, 255, 255);
}

/*模版*/

.swiper {
  width: 100%;
}

.sw_txt {
  padding: 4rpx 6rpx;
  position: absolute;
  top: 0;
  right: 20rpx;
  font-size: 25.4rpx;
  color: rgb(255, 255, 255);
  border-radius: 2rpx;
}

.item_css {
  padding: 0rpx 28rpx 49rpx 33rpx;
  position: relative;
}

.item_text {
  font-size: 34rpx;
  line-height: 50rpx;
  letter-spacing: -0.8rpx;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.item_time {
  font-size: 26rpx;
  color: rgb(191, 191, 191);
  margin-top: 10rpx;
}

.item_img {
  height: 41rpx;
  width: 41rpx;
  border: 1rpx solid rgb(219, 219, 219);
  border-radius: 20.5rpx;
}

.item_text_ab {
  font-size: 26rpx;
  color: rgb(191, 191, 191);
  padding-left: 10.8rpx;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  overflow: hidden;
  width: 340rpx;
}

.item_price {
  font-size: 40rpx;
  color: rgb(61, 209, 164);
  top: -4rpx;
  position: absolute;
  right: 0;
}

.mangage-club-content {
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: relative;
}

.mangage-club-btn {
  position: absolute;
  right: 4%;
  color: #3dd1a5;
  border: 2rpx solid #3dd1a5;
  border-radius: 10rpx;
  padding: 14rpx 20rpx;
  font-size: 30rpx;
}

.line2 {
  display: flex;
  align-items: center;
  margin-bottom: 26rpx;
}

.line2_img {
  width: 36rpx;
  height: 31rpx;
  margin-right: 16rpx;
}

.kuangkuang {
  height: 42rpx;
  width: 85rpx;
  border-radius: 6rpx;
  margin-right: 12rpx;
  line-height: 42rpx;
}

.icon_text {
  font-size: 26rpx;
  text-align: center;
  line-height: 42rpx;
  margin-right: 20rpx;
  width: 85rpx;
  height: 42rpx;
  color: white;
  border-radius: 10rpx;
}

.border_1 {
  font-size: 24rpx;
  color: rgb(142, 142, 142);
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: white;
  height: 130rpx;
}

.border_1_1 {
  border-bottom: 1rpx solid rgb(230, 230, 230);
  width: 130rpx;
}

.club-index-title {
  display: flex;
}

.txt-overflow {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.master-project-overflow {
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.items-tap {
  display: flex;
  align-items: center;
  justify-content: center;
  white-space: nowrap;
}

.container_title {
  padding-bottom: 0;
}

.index-tip {
  text-align: center;
  line-height: 40rpx;
  width: 40rpx;
  height: 40rpx;
  color: white;
  background-color: red;
  border-radius: 50%;
  font-size: 26rpx;
  margin-right: 20rpx;
}

.flex-end {
  display: flex;
  align-items: center;
  align-content: center;
  justify-content: flex-end;
  padding-right: 30rpx;
}

.flex-between {
  display: flex;
  align-items: center;
  align-content: center;
  justify-content: space-between;
}

.club-index-detail {
  flex-direction: column;
  display: flex;
  margin-left: 28rpx;
}
.club-index-list{
  display:flex;
  justify-content:space-between;
  align-items:center;
  height: 42rpx;
  margin-bottom:19rpx;
  margin-top:15rpx;
}