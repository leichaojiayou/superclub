/* pages/clubhome/clubhome.wxss */
@import "../common1/common.wxss";
.home_head{
    display: flex;
    flex-direction: row;
    background-color: rgb(37, 37, 37);
    height: 150px;
    padding-top: 15px;
}
.club_img{
    width: 70px;
    height: 70px;
        flex-shrink: 0;

}
.head_first{
    margin-left: 20px;
    margin-right: 15px;
     flex-shrink: 0;
}
.head_second{
    padding-top: 5px;
    width: 50%;

}
.second_label{
    color: rgb(255, 255, 255);
    font-size: 13px;
    padding: 5px 10px 5px 10px;
    background-color: rgb(145,213,192);
    border-radius: 3px;
    margin-right: 10px;
}
.second_des{
    margin-top: 10px;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    line-height:20px; 
}
.head_thrid{
    padding-top: 65px;
    margin-right: 15px;
}
.head_QRcode{
    height: 20px;
    width: 20px;
}
.home_list{
     display:flex; 
     flex-direction: row;
     justify-content: space-around;
     margin: 20px 0 25px 0;
}
.home_list_one{
    display: flex;
    align-items: center;
}


.tran {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  position: relative;
}

.line {
  position: absolute;
  width: 80px;
  border-bottom: 1px solid rgb(230, 230, 230);
  top: 9px;
}