<!-- pages/julebumingcheng/julebumingcheng.wxml -->
<form bindsubmit="formSubmit">
    <view class="header">
        <input name="title" placeholder="俱乐部名称15字以内" placeholder-class="t" value="{{clubName}}" bindinput="change"  />
        <icon type="clear" size="18" bindtap="cleanValue"></icon>
    </view>
    <button style="margin:60rpx 30rpx 0 30rpx;" type="primary" form-type="submit" wx:if="{{clubName}}">保存</button>
    <button style="margin:60rpx 30rpx 0 30rpx;" type="primary" disabled="true"wx:else>保存</button>
</form>