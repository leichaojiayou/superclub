<!--pages/dismiss/dismiss.wxml-->
<view wx:if="{{status==0}}">
<view class="header">
<image src="{{header_img}}" class="header_img"></image>
</view>
<view class="header_t">
<text style="font-size:26rpx;Letter-spacing:-0.6rpx;color:rgb(191,191,191)">副会长</text>
</view>
<view class="header_btn">
<button type="primary">设置副会长</button>
</view></view>



<view wx:if="{{status==1}}">
<view class="header">
<image src="{{header_img}}" class="header_img"></image>
</view>
<view class="header_t">
<text style="font-size:26rpx;Letter-spacing:-0.6rpx;color:rgb(191,191,191)">管理员</text>
</view>
<view class="header_btn">
<button type="primary" bindtap="nato_removes">添加管理员</button>
</view></view>



<view wx:if="{{status==2}}">
<view class="header">
<image src="{{header_img}}" class="header_img"></image>
</view>
<view class="header_t">
<text style="font-size:32rpx;Letter-spacing:-0.8rpx;">{{status3_name}}</text>
</view>
<view class="header_btn">
<button type="primary" bindtap="nato_removes" disabled="true">添加管理员</button>
<view style="margin-top:29rpx;">
<button>移除副会长</button>
</view>
</view></view>