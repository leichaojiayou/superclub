@import "../../../template/toasts/toasts.wxss";
page {
  font-size: 30rpx;
  color: rgb(26, 26, 26);
}

/*一个大框*/

.bigview {
  margin: 30rpx 0 0 0;
  display: flex;
  flex-direction: column;
  align-items: stretch;
  background-color: white;
  border-top: 1rpx solid rgb(230, 230, 230);
  border-bottom: 1rpx solid rgb(230, 230, 230);
}

/*大框里面item框*/

/*会长 副会长*/

.itemView {
  margin-left: 29rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 90rpx;
  border-bottom: 1rpx solid rgb(230, 230, 230);
}

.item-view-show {
  margin-left: 29rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid rgb(230, 230, 230);
}

/*管理员 会员 非会员*/

.itemView1 {
  margin-left: 30rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  min-height: 88rpx;
  border-bottom: 1rpx solid rgb(230, 230, 230);
}

/*管理员 会员 非会员 宣言  简介*/

.itemView2 {
  margin-top: 29rpx;
  margin-left: 30rpx;
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  min-height: 88rpx;
  border-bottom: 1rpx solid rgb(230, 230, 230);
}

/*会长 副会长*/

/*item框中的脚*/

.itemView_foot {
  margin-right: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

/*管理员 会员 非会员*/

/*item框中的开头部*/

.itemView_hear {
  width: 181rpx;
}

/*item框中的尾部*/

.itemView_foot1 {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

/*大字体*/

.bigsize {
  font-size: 34rpx;
}

.bigsize_2 {
  font-size: 34rpx;
  display:  -webkit-box;
  -webkit-line-clamp:  1;
  -webkit-box-orient:  vertical;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 431rpx;
  text-align: right;
}

.bigsize_3 {
  width: 10%;
  font-size: 34rpx;
  display:  -webkit-box;
  -webkit-line-clamp:  1;
  -webkit-box-orient:  vertical;
  overflow: hidden;
  text-overflow: ellipsis;
}

.bigsize_1 {
  font-size: 34rpx;
}

.bigsize1 {
  margin-bottom: 31rpx;
  font-size: 34rpx;
  width: 474rpx;
  word-wrap: break-word;
  white-space: normal;
}

/*俱乐部logo*/

.logo_image {
  width: 130rpx;
  height: 130rpx;
  border-radius: 10rpx;
}

/*星级图标*/

.start_image {
  width: 32rpx;
  height: 31rpx;
  margin-right: 18rpx;
}

/*俱乐部成员统计icon图标*/

.statistics_icon {
  width: 58rpx;
  height: 58rpx;
}

/*文字类似图标*/

.icon_text {
  font-size: 26rpx;
  text-align: center;
  line-height: 42rpx;
  margin-left: 20rpx;
  width: 85rpx;
  height: 42rpx;
  color: white;
  border-radius: 10rpx;
}

.icon_text1 {
  font-size: 26rpx;
  text-align: center;
  line-height: 42rpx;
  margin-right: 20rpx;
  width: 85rpx;
  height: 42rpx;
  color: limegreen;
  border: 1px solid;
  border-radius: 10rpx;
}

/*文字圆图标*/

.mgs_text {
  text-align: center;
  font-size: 25rpx;
  line-height: 36rpx;
  width: 36rpx;
  height: 36rpx;
  color: white;
  background-color: red;
  border-radius: 50%;
}

.admin_text {
  margin-left: 20rpx;
  text-align: center;
  font-size: 26rpx;
  line-height: 54rpx;
  width: 54rpx;
  height: 54rpx;
  color: rgb(142, 142, 142);
  background-color: rgb(241, 241, 241);
  border-radius: 50%;
  border: 1rpx solid rgb(219, 219, 219);
}

/*itemView_foot*/

/*修改过的foot_text_view*/

.foot_text_view {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  white-space: nowrap;
  width: 426rpx;
  margin-right: 30rpx;
}

.foot-text-view-none {
  margin-right: 31rpx;
}

.last_foot_view {
  width: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

/*按钮框*/

.buttonview {
  width: 100%;
  margin-top: 59rpx;
  margin-bottom: 167rpx;
}

.buttonview button {
  font-size: 18px;
  border: 1rpx solid rgb(214, 73, 73);
  background-color: rgb(233, 79, 79);
  color: white;
  width: 690rpx;
}

.club-foot-show-text {
  white-space: normal;
  word-break: break-all;
  word-wrap: break-word;
  line-height: 50rpx;
}

.club-count-member {
  display: flex;
  flex-direction: row;
  margin-right: 30rpx;
  height: 128rpx;
}

.club-count {
  margin-right: 32%;
  width: 251rpx;
}

.item-show-removeBrode {
  border-bottom: 0rpx;
}

.item-short-txt {
  padding: 33rpx 32rpx 30rpx 0rpx;
}

.master-project-overflow{
  overflow : hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
