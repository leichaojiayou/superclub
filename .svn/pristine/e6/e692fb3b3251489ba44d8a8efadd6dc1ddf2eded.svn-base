page {
  color: black;
}

.mid_view {
  font-size: 34rpx;
}

.checkbox_text_view {
  margin-left: 30rpx;
}

.checkbox_view {
  margin-top: 10rpx;
  background-color: rgb(246, 246, 246);
  display: flex;
  align-items: center;
}

.group_icon {
  margin-top: 15rpx;
  margin-left: 25rpx;
  margin-bottom: 15rpx;
}

.item_icon {
  margin-top: 32rpx;
  margin-left: 25rpx;
  margin-bottom: 32rpx;
}

.item_mid_view {
  margin-left: 30rpx;
  width: 100%;
  height: 100%;
  display: flex;
  flex-shrink: 0;
  align-items: center;
  border-bottom: 0.5px solid rgb(230, 230, 230);
}

.group_view {
  border-top: 0.5px solid rgb(230, 230, 230);
  border-bottom: 0.5px solid rgb(230, 230, 230);
  background-color: rgb(255, 255, 255);
}

.checkbox_group_view {
  background-color: rgb(255, 255, 255);
  height: 150rpx;
  display: flex;
  align-items: center;
}

.item_img {
  width: 80rpx;
  height: 80rpx;
}

.item_right_view {
  margin-left: 39rpx;
  margin-right: 32rpx;
  display: flex;
  flex-shrink: 0;
  flex-direction: column;
}

.user_info_name_top {
  display: flex;
  align-items: baseline;
  justify-content: space-between;
}

.smart_text_size {
  margin-top: 25rpx;
  font-size: 24rpx;
  color: rgb(142, 142, 142);
}

.smartsize {
  font-size: 24rpx;
  color: rgb(142, 142, 142);
}

.foot_text {
  font-size: 32rpx;
  width: 50%;
  border-right: 1rpx solid rgb(230, 230, 230);
}

.foot_view {
  height: 8%;
  background-color: rgb(255, 255, 255);
  color: rgb(61, 209, 164);
  width: 100%;
  display: flex;
  text-align: center;
  align-items: center;
  justify-content: space-between;
  position: absolute;
  bottom: 0px;
}
