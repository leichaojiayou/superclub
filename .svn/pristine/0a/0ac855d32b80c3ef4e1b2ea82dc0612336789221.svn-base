const api = require("./request");
class User {
    constructor() {
        console.log('user api');
    }
    /**
     * 小程序登陆: http://doc.iweju.com/FitNesse.WejuBaHttp.BindDoc.UserWxLoginDoc
     * 请求参数： code， encryptedData，iv
     */
    login(param) {
        api.wxRequest(param, "/ba/user/wxlogin")
    }


    /**
     * 查看自己参与的俱乐部列表: http://doc.iweju.com/FitNesse.WejuBaHttp.ClubDoc.ClubHomeListDoc
     */
    joinedClubs(param) {
        api.wxRequest(param, "/ba/club/home/list")
    }
    /**
     * 微信绑定账号的检查: 
     */
    phoneCheck(param) {
        api.wxRequest(param, "/ba/account/phone/login/check")
    }

    /**
     * 我加入的俱乐部: http://doc.iweju.com/FitNesse.WejuBaHttp.ClubDoc.ClubHomeListDoc
     */
    myClubs(params) {
        api.wxRequest(params, "/ba/club/home/list")
    }
}

module.exports = User