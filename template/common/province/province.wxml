<!--template/common/province/province.wxml-->
<view class="place-page">
  <view  wx:for="{{datas}}" wx:key="place">
    <view class="place-content" catchtap="chooseProvince" data-province="{{item.province}}">
      <text class="palce-content-text">{{item.province}}</text>
      <image class="palce-content-icon" mode="widthFix" src="https://cdn.51julebu.com/xiaochengxu/image/right-errow.png"></image>
    </view>
  </view>
</view>
