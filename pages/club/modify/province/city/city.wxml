<!--template/common/city/city.wxml-->
<view class="place-page">
  <view wx:for="{{cities}}" wx:key="city">
    <view class="place-content"  data-cityName="{{item.cityName}}" data-cityid="{{item.cityID}}" catchtap="chooseCity">
      <text class="palce-content-text">{{item.cityName}}</text>
      <image class="right-arrow-absolute" style="top:28rpx" mode="aspectFit" src="https://cdn.51julebu.com/xiaochengxu/image/black-errow@2x.png"></image>
    </view>
  </view>
</view>


