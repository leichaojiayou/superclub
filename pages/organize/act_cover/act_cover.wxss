/* pages/huodongfengmian/huodongfengmian.wxss */
page {
  word-wrap: break-word;
  white-space: normal;
  background-color: rgb(255, 255, 255);
}
.t34rpx{
    font-size: 34rpx;
    letter-spacing: -0.8rpx;
}
.body{
   
    margin-top: 50rpx;
    height: 436rpx;

}
.body_item1{
     position: relative;
    border-top: 1rpx solid rgb(230, 230, 230);
    height: 50rpx;
    margin: 0 177rpx 0 177rpx;
}
.txt_position{
    display: flex;
    justify-content: center;
    position: absolute;
    width: 136rpx;
    height: 32rpx;
    top: -17rpx;
    right: 136rpx;
    background-color: rgb(255, 255, 255);
}
/*图片循环样式*/

.body_item{
    height: 386rpx;
    flex-wrap: wrap;
    display: flex;
    justify-content: space-around;
    align-items: center;
}
.img{
    height: 108rpx;
    width: 230.4rpx;
}

/*图片预览*/

.swiper-container {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: #000;
}

.swiper-container image {
  width: 100%;
  height: 100%;
}
